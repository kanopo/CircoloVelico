<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.AnchorPane?>
<AnchorPane prefHeight="600.0" prefWidth="800.0" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="me.ollari.circolovelicogui.controllers.employeeFunctionality.RaceFeeHistory">
    <children>
        <Button layoutX="14.0" layoutY="14.0" minHeight="25.0" minWidth="150.0" mnemonicParsing="false"
                onAction="#go_home" text="Home"/>
        <Button layoutX="636.0" layoutY="14.0" minHeight="25.0" minWidth="150.0" mnemonicParsing="false"
                onAction="#logout" text="Logout"/>
        <Label layoutX="344.0" layoutY="19.0" text="Race fees history"/>
        <TableView fx:id="raceFeeHistoryTable" layoutY="78.0" prefHeight="521.0" prefWidth="800.0">
            <columns>
                <TableColumn fx:id="id" editable="false" prefWidth="133.0" text="ID"/>
                <TableColumn fx:id="memberId" editable="false" prefWidth="133.0" text="Member ID"/>
                <TableColumn fx:id="boatId" editable="false" prefWidth="133.0" text="Boat ID"/>
                <TableColumn fx:id="raceId" editable="false" prefWidth="133.0" text="Race ID"/>
                <TableColumn fx:id="price" editable="false" prefWidth="133.0" text="Price"/>
                <TableColumn fx:id="paymentDate" editable="false" prefWidth="133.0" text="Data pagamento"/>
            </columns>
        </TableView>
    </children>
</AnchorPane>
