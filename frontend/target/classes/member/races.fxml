<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.AnchorPane?>
<AnchorPane prefHeight="400.0" prefWidth="801.0" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="me.ollari.circolovelicogui.controllers.memberFunctionality.MemberRaces">
    <children>
        <Label layoutX="353.0" layoutY="19.0" text="Tabellone gare"/>
        <TableView fx:id="raceTable" layoutY="45.0" onMouseReleased="#selectBoat" prefHeight="300.0" prefWidth="803.0">
            <columns>
                <TableColumn fx:id="name" prefWidth="200.0" text="Nome"/>
                <TableColumn fx:id="date" prefWidth="200.0" text="Data"/>
                <TableColumn fx:id="price" prefWidth="200.0" text="Prezzo"/>
                <TableColumn fx:id="award" prefWidth="200.0" text="Vincita"/>
            </columns>
        </TableView>
        <Button layoutX="14.0" layoutY="14.0" minHeight="25.0" minWidth="150.0" mnemonicParsing="false"
                onAction="#go_home" prefHeight="20.0" prefWidth="100.0" text="Home"/>
        <Button layoutX="639.0" layoutY="14.0" minHeight="25.0" minWidth="150.0" mnemonicParsing="false"
                onAction="#logout" prefHeight="20.0" prefWidth="100.0" text="Logout"/>
        <Button layoutX="639.0" layoutY="362.0" minHeight="25.0" minWidth="150.0" mnemonicParsing="false"
                onAction="#IscriviBarca" text="Iscrivi barca"/>
        <ComboBox fx:id="boatSelector" layoutX="14.0" layoutY="362.0" minHeight="25.0" minWidth="150.0"
                  prefHeight="24.0" prefWidth="210.0" promptText="Selezionare barca"/>
    </children>
</AnchorPane>
