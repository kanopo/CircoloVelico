<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/18" xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="me.ollari.circolonauticogui.controllers.employeeFunctionality.AddMember">
    <children>
        <Label layoutX="169.0" layoutY="21.0" text="Inserisci i dati dell'utente da aggiungere"/>
        <Button layoutX="14.0" layoutY="17.0" mnemonicParsing="false" onAction="#go_home" text="Home"/>
        <Button layoutX="524.0" layoutY="17.0" mnemonicParsing="false" onAction="#logout" text="Logout"/>
        <VBox layoutY="48.0" prefHeight="271.0" prefWidth="412.0">
            <children>
                <HBox alignment="CENTER_RIGHT" prefHeight="54.0" prefWidth="600.0">
                    <children>
                        <Label text="Nome">
                            <HBox.margin>
                                <Insets bottom="10.0" left="10.0" right="10.0" top="10.0"/>
                            </HBox.margin>
                        </Label>
                        <TextField fx:id="name">
                            <HBox.margin>
                                <Insets bottom="10.0" left="10.0" right="10.0" top="10.0"/>
                            </HBox.margin>
                        </TextField>
                    </children>
                </HBox>
                <HBox alignment="CENTER_RIGHT" prefHeight="54.0" prefWidth="600.0">
                    <children>
                        <Label text="Cognome">
                            <HBox.margin>
                                <Insets bottom="10.0" left="10.0" right="10.0" top="10.0"/>
                            </HBox.margin>
                        </Label>
                        <TextField fx:id="surname">
                            <HBox.margin>
                                <Insets bottom="10.0" left="10.0" right="10.0" top="10.0"/>
                            </HBox.margin>
                        </TextField>
                    </children>
                </HBox>
                <HBox alignment="CENTER_RIGHT" prefHeight="54.0" prefWidth="600.0">
                    <children>
                        <Label text="Indirizzo(Casa)">
                            <HBox.margin>
                                <Insets bottom="10.0" left="10.0" right="10.0" top="10.0"/>
                            </HBox.margin>
                        </Label>
                        <TextField fx:id="address">
                            <HBox.margin>
                                <Insets bottom="10.0" left="10.0" right="10.0" top="10.0"/>
                            </HBox.margin>
                        </TextField>
                    </children>
                </HBox>
                <HBox alignment="CENTER_RIGHT" prefHeight="54.0" prefWidth="600.0">
                    <children>
                        <Label text="Codice fiscale">
                            <HBox.margin>
                                <Insets bottom="10.0" left="10.0" right="10.0" top="10.0"/>
                            </HBox.margin>
                        </Label>
                        <TextField fx:id="fiscal_code">
                            <HBox.margin>
                                <Insets bottom="10.0" left="10.0" right="10.0" top="10.0"/>
                            </HBox.margin>
                        </TextField>
                    </children>
                </HBox>
                <HBox alignment="CENTER_RIGHT" prefHeight="54.0" prefWidth="600.0">
                    <children>
                        <Label text="Username">
                            <HBox.margin>
                                <Insets bottom="10.0" left="10.0" right="10.0" top="10.0"/>
                            </HBox.margin>
                        </Label>
                        <TextField fx:id="username">
                            <HBox.margin>
                                <Insets bottom="10.0" left="10.0" right="10.0" top="10.0"/>
                            </HBox.margin>
                        </TextField>
                    </children>
                </HBox>
                <HBox alignment="CENTER_RIGHT" prefHeight="54.0" prefWidth="600.0">
                    <children>
                        <Label text="Password">
                            <HBox.margin>
                                <Insets bottom="10.0" left="10.0" right="10.0" top="10.0"/>
                            </HBox.margin>
                        </Label>
                        <TextField fx:id="password">
                            <HBox.margin>
                                <Insets bottom="10.0" left="10.0" right="10.0" top="10.0"/>
                            </HBox.margin>
                        </TextField>
                    </children>
                </HBox>
            </children>
        </VBox>
        <Button layoutX="202.0" layoutY="341.0" mnemonicParsing="false" onAction="#add_member"
                text="Aggiungi membro al circolo"/>
    </children>
</AnchorPane>
